<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="solutie.hbm">

    <class name="model.Solutie" table="Solutii">
        <id name="id" type="long" column="id_solutie">
            <generator class="increment"/>
        </id>
        <property name="rezolvare" type="string" column="rezolvare"/>
<!--        <one-to-one name="bugRezolvat" class="model.Bug"/>-->
<!--        one-to-one nu merge insert in baza de date dar merge select-->
        <many-to-one name="bugRezolvat" class="model.Bug" column="id_bug" unique="true"/>
<!--            many-to-one merge insert in baza de date dar nu merge select-->
    </class>

</hibernate-mapping>
